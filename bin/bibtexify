#!/bin/bash

# Test for a master bibliography
[ -f $1.aux ] && bibtex $1

# Test for a bibliography 1
[ -f ${1}1.aux ] && bibtex ${1}1

# Test for a bibliography 2
[ -f ${1}2.aux ] && bibtex ${1}2

# Test for a bibliography 3
[ -f ${1}3.aux ] && bibtex ${1}3

# Test for a bibliography 4
[ -f ${1}4.aux ] && bibtex ${1}4

# Test for a bibliography 5
[ -f ${1}5.aux ] && bibtex ${1}5

# Test for a bibliography 6
[ -f ${1}6.aux ] && bibtex ${1}6

# Test for a bibliography 7
[ -f ${1}7.aux ] && bibtex ${1}7
