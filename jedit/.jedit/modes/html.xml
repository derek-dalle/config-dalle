	<!-- INLINE PHP -->
	<RULES SET="INLINE_PHP" DEFAULT="MARKUP">
		<SPAN TYPE="COMMENT2" EXCLUDE_MATCH="FALSE">
			<BEGIN>&lt;!--[</BEGIN>
			<END>&gt;</END>
		</SPAN>
		<SPAN TYPE="COMMENT2" EXCLUDE_MATCH="FALSE">
			<BEGIN>&lt;![</BEGIN>
			<END>--&gt;</END>
		</SPAN>
		<SPAN TYPE="COMMENT1" EXCLUDE_MATCH="FALSE">
			<BEGIN>&lt;!--</BEGIN>
			<END>--&gt;</END>
		</SPAN>		
		<SPAN TYPE="MARKUP" DELEGATE="php::PHP">
			<BEGIN>&lt;?php</BEGIN>
			<END>?&gt;</END>
		</SPAN>
		<SPAN TYPE="MARKUP" DELEGATE="php::PHP">
			<BEGIN>&lt;?=</BEGIN>
			<END>?&gt;</END>
		</SPAN>
