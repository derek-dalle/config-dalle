<?xml version="1.0"?>
<!DOCTYPE MODE SYSTEM "xmode.dtd">
<!-- 
	A COLORFUL HTML 1.1
	(a jEdit 4.3 mode definition)
	by Venimus (venimus@gmail.com)

-->
<MODE>
	<PROPS>
		<PROPERTY NAME="commentStart" VALUE="&lt;!--" />
		<PROPERTY NAME="commentEnd" VALUE="--&gt;" />
	</PROPS>

	<!-- MAIN -->
	<RULES IGNORE_CASE="TRUE" NO_WORD_SEP="-"
			HIGHLIGHT_DIGITS="TRUE">
		<!-- SSI -->
		<!-- SSI directives should ALWAYS end like this, with a space
			before the tag end, so let's make it colorize poorly otherwise -->
		<!-- ... or really? see bug report 964860 -->
		<SPAN TYPE="KEYWORD3" DELEGATE="SSI">
			<BEGIN>&lt;!--#</BEGIN>
			<END>--&gt;</END>
		</SPAN>
		
			<!-- SGML entities -->
		<SPAN TYPE="LITERAL2" NO_WORD_BREAK="TRUE">
			<BEGIN>&amp;</BEGIN>
			<END>;</END>
		</SPAN>
		<SPAN TYPE="FUNCTION" EXCLUDE_MATCH="FALSE">
			<BEGIN>&lt;!DOCTYPE</BEGIN>
			<END>&gt;</END>
		</SPAN>	
		
		<SEQ_REGEXP TYPE="DIGIT" HASH_CHAR="#">(#[0-9a-fA-F]{6})|(#[0-9a-fA-F]{3})</SEQ_REGEXP>
		<SEQ_REGEXP TYPE="DIGIT" HASH_CHAR="-">-\d*\.{0,1}\d+%?</SEQ_REGEXP>
		<SEQ_REGEXP TYPE="DIGIT" HASH_CHAR="+">\+\d*\.{0,1}\d+%?</SEQ_REGEXP>
		<SEQ_REGEXP TYPE="DIGIT" HASH_CHAR=".">\.{0,1}\d+%?</SEQ_REGEXP>
		<SEQ_REGEXP TYPE="DIGIT" HASH_CHAR="1">1\.{0,1}\d+%?</SEQ_REGEXP>
		<SEQ_REGEXP TYPE="DIGIT" HASH_CHAR="2">2\.{0,1}\d+%?</SEQ_REGEXP>
		<SEQ_REGEXP TYPE="DIGIT" HASH_CHAR="3">3\.{0,1}\d+%?</SEQ_REGEXP>
		<SEQ_REGEXP TYPE="DIGIT" HASH_CHAR="4">4\.{0,1}\d+%?</SEQ_REGEXP>
		<SEQ_REGEXP TYPE="DIGIT" HASH_CHAR="5">5\.{0,1}\d+%?</SEQ_REGEXP>
		<SEQ_REGEXP TYPE="DIGIT" HASH_CHAR="6">6\.{0,1}\d+%?</SEQ_REGEXP>
		<SEQ_REGEXP TYPE="DIGIT" HASH_CHAR="7">7\.{0,1}\d+%?</SEQ_REGEXP>
		<SEQ_REGEXP TYPE="DIGIT" HASH_CHAR="8">8\.{0,1}\d+%?</SEQ_REGEXP>
		<SEQ_REGEXP TYPE="DIGIT" HASH_CHAR="9">9\.{0,1}\d+%?</SEQ_REGEXP>
		
		<SPAN TYPE="COMMENT2" EXCLUDE_MATCH="FALSE">
			<BEGIN>&lt;!--[</BEGIN>
			<END>&gt;</END>
		</SPAN>
		<SPAN TYPE="COMMENT2" EXCLUDE_MATCH="FALSE">
			<BEGIN>&lt;![</BEGIN>
			<END>--&gt;</END>
		</SPAN>
		<SPAN TYPE="COMMENT1" EXCLUDE_MATCH="FALSE">
			<BEGIN>&lt;!--</BEGIN>
			<END>--&gt;</END>
		</SPAN>		
		<SPAN TYPE="MARKUP" DELEGATE="php::PHP">
			<BEGIN>&lt;?php</BEGIN>
			<END>?&gt;</END>
		</SPAN>
		<SPAN TYPE="MARKUP" DELEGATE="php::PHP">
			<BEGIN>&lt;?=</BEGIN>
			<END>?&gt;</END>
		</SPAN>
		<SPAN TYPE="FUNCTION">
			<BEGIN>&lt;?xml</BEGIN>
			<END>?&gt;</END>
		</SPAN>
		<SPAN TYPE="MARKUP" DELEGATE="php::PHP">
			<BEGIN>&lt;?</BEGIN>
			<END>?&gt;</END>
		</SPAN>
		<SPAN_REGEXP HASH_CHAR="&lt;" TYPE="KEYWORD1" DELEGATE="OPEN_CSS_TAG_ATTRIBUTE_CONTENT">
			<BEGIN>&lt;[\s$]*style</BEGIN>
			<END>&lt;/style&gt;</END>
		</SPAN_REGEXP>
		<SPAN_REGEXP HASH_CHAR="&lt;" TYPE="KEYWORD1" DELEGATE="OPEN_SCRIPT_TAG_ATTRIBUTE_CONTENT">
			<BEGIN>&lt;[\s$]*script</BEGIN>
			<END>&lt;/script&gt;</END>
		</SPAN_REGEXP>
		<SPAN_REGEXP HASH_CHAR="&lt;" TYPE="MARKUP" DELEGATE="CLOSE_TAG_CONTENT">
			<BEGIN>&lt;[\s$]*/[\s$]*</BEGIN>
			<END>&gt;</END>
		</SPAN_REGEXP>
		<SPAN_REGEXP HASH_CHAR="&lt;" TYPE="MARKUP" DELEGATE="OPEN_TAG_CONTENT">
			<BEGIN>&lt;[\s$]*</BEGIN>
			<END>&gt;</END>
		</SPAN_REGEXP>

	</RULES>
	<RULES DEFAULT="MARKUP" SET="OPEN_TAG_CONTENT">
		<IMPORT DELEGATE="INLINE_PHP"/>
		<IMPORT DELEGATE="OPEN_TAGS"/>
		<SEQ TYPE="INVALID" DELEGATE="INVALID">=</SEQ>
		<SPAN_REGEXP HASH_CHAR="s" AT_WHITESPACE_END="TRUE" TYPE="LITERAL2" DELEGATE="css::BLOCK">
			<BEGIN>style[\s$]*=[\s$]*(&quot;|'|$)</BEGIN>
			<END>$1|'</END>
		</SPAN_REGEXP>
		<SPAN_REGEXP HASH_CHAR="s" AT_WHITESPACE_END="TRUE" TYPE="INVALID" DELEGATE="OPEN_TAG_CONTENT">
			<BEGIN>script[\s$]*(.*)</BEGIN>
			<END>$1</END>
		</SPAN_REGEXP>
		
		<SEQ TYPE="FUNCTION" DELEGATE="OPEN_TAG_ATTRIBUTE_CONTENT">&#09;</SEQ>
		<SEQ TYPE="FUNCTION" DELEGATE="OPEN_TAG_ATTRIBUTE_CONTENT">&#32;</SEQ>
		<SEQ TYPE="FUNCTION" DELEGATE="OPEN_TAG_ATTRIBUTE_CONTENT">&#13;</SEQ>
		<KEYWORDS>
			<KEYWORD1>style</KEYWORD1>
		</KEYWORDS>
		
	</RULES>               
	<RULES DEFAULT="INVALID" SET="CLOSE_TAG_CONTENT" >
		<IMPORT DELEGATE="INLINE_PHP"/>
		<IMPORT DELEGATE="CLOSE_TAGS"/>
		<KEYWORDS>
			<KEYWORD1>style</KEYWORD1>
		</KEYWORDS>
	</RULES>
	
	
	
	<RULES DEFAULT="INVALID" SET="INVALID">
		<IMPORT DELEGATE="INLINE_PHP"/>
	</RULES>

	
	
	
	<RULES DEFAULT="FUNCTION" SET="OPEN_TAG_ATTRIBUTE_CONTENT">
		<IMPORT DELEGATE="INLINE_PHP"/>
		<IMPORT DELEGATE="ATTRIBUTES"/>
		<SPAN_REGEXP HASH_CHAR="s" TYPE="KEYWORD4" DELEGATE="css::BLOCK">
			<BEGIN>style[\s]*=[\s]*(&quot;|')</BEGIN>
			<END>$1</END>
		</SPAN_REGEXP>
		<SPAN_REGEXP HASH_CHAR="o" TYPE="LITERAL3" DELEGATE="javascript::MAIN">
			<BEGIN>(?:onblur|onchange|onclick|ondblclick|onfocus|onkeydown|onkeypress|onkeyup|onload|onmousedown|onmouseenter|onmousemove|onmouseout|onmouseover|onmouseup|onreset|onsubmit|onunload)[\s$]*=[\s$]*(&quot;|'|$)</BEGIN>
			<END>$1</END>
		</SPAN_REGEXP>
		
		<SEQ_REGEXP HASH_CHAR="=" TYPE="OPERATOR" DELEGATE="OPEN_TAG_ATTRIBUTE_VALUE_CONTENT">=[\s$]*</SEQ_REGEXP>
		<SEQ TYPE="OPERATOR" DELEGATE="OPEN_TAG_ATTRIBUTE_VALUE_CONTENT">=</SEQ>
		<SEQ TYPE="MARKUP" DELEGATE="INVALID">/</SEQ>
		<SEQ TYPE="LITERAL1" DELEGATE="INVALID">"</SEQ>
		<SEQ TYPE="LITERAL1" DELEGATE="INVALID">'</SEQ>
	</RULES>
	
	<RULES DEFAULT="LITERAL2" SET="OPEN_TAG_ATTRIBUTE_VALUE_CONTENT">
		<IMPORT DELEGATE="INLINE_PHP"/>
		
		
   
		<SEQ_REGEXP HASH_CHAR="&#32;" TYPE="LITERAL2" DELEGATE="OPEN_TAG_ATTRIBUTE_VALUE_CONTENT1">[\s$]*&quot;</SEQ_REGEXP>
		<SEQ_REGEXP HASH_CHAR="&#09;" TYPE="LITERAL2" DELEGATE="OPEN_TAG_ATTRIBUTE_VALUE_CONTENT1">[\s$]*&quot;</SEQ_REGEXP>
		
		<SEQ_REGEXP HASH_CHAR="&#32;" TYPE="LITERAL1" DELEGATE="OPEN_TAG_ATTRIBUTE_VALUE_CONTENT2">[\s$]*'</SEQ_REGEXP>
		<SEQ_REGEXP HASH_CHAR="&#09;" TYPE="LITERAL1" DELEGATE="OPEN_TAG_ATTRIBUTE_VALUE_CONTENT2">[\s$]*'</SEQ_REGEXP>
		
		
		
		<SEQ TYPE="LITERAL2" DELEGATE="OPEN_TAG_ATTRIBUTE_VALUE_CONTENT1">&quot;</SEQ>
		<SEQ TYPE="LITERAL1" DELEGATE="OPEN_TAG_ATTRIBUTE_VALUE_CONTENT2">'</SEQ>
		
		<SEQ TYPE="INVALID" DELEGATE="OPEN_TAG_CONTENT">=</SEQ>
		<SEQ_REGEXP HASH_CHAR="&#32;" TYPE="INVALID">[\s$]*=</SEQ_REGEXP>
		<SEQ_REGEXP HASH_CHAR="&#09;" TYPE="INVALID">[\s$]*=</SEQ_REGEXP>
		<!-- handle another type of inline javascript -->
		<SEQ TYPE="LITERAL4" DELEGATE="javascript::MAIN">javascript:</SEQ>
		
		<SEQ TYPE="LITERAL1" DELEGATE="OPEN_TAG_ATTRIBUTE_CONTENT">&#32;</SEQ>
		<SEQ TYPE="LITERAL1" DELEGATE="OPEN_TAG_ATTRIBUTE_CONTENT">&#09;</SEQ>
		
		
		
		<SPAN_REGEXP HASH_CHAR="&quot;" TYPE="INVALID">
			<BEGIN>"</BEGIN>
			<END>"</END>
		</SPAN_REGEXP>
		<SPAN_REGEXP HASH_CHAR="'" TYPE="INVALID">
			<BEGIN>'</BEGIN>
			<END>'</END>
		</SPAN_REGEXP>
	</RULES>
	
	<RULES DEFAULT="LITERAL2" SET="OPEN_TAG_ATTRIBUTE_VALUE_CONTENT1">
		<IMPORT DELEGATE="INLINE_PHP"/>
		<!-- handle another type of inline javascript -->
		<SEQ TYPE="LITERAL4" AT_WORD_START="TRUE" DELEGATE="JS1">javascript</SEQ>
		<SEQ TYPE="LITERAL2" DELEGATE="OPEN_TAG_ATTRIBUTE_CONTENT">&quot;&#32;</SEQ>
		<SEQ TYPE="LITERAL2" DELEGATE="OPEN_TAG_ATTRIBUTE_CONTENT">&quot;&#09;</SEQ>
	</RULES>
	
	<RULES DEFAULT="LITERAL3" SET="OPEN_TAG_ATTRIBUTE_VALUE_CONTENT2">
		<IMPORT DELEGATE="INLINE_PHP"/>
		<!-- handle another type of inline javascript -->
		<SEQ TYPE="LITERAL4" AT_WORD_START="TRUE" DELEGATE="JS2">javascript</SEQ>
		<SEQ TYPE="LITERAL3" DELEGATE="OPEN_TAG_ATTRIBUTE_CONTENT">'&#32;</SEQ>
		<SEQ TYPE="LITERAL3" DELEGATE="OPEN_TAG_ATTRIBUTE_CONTENT">'&#09;</SEQ>
	</RULES>
	
	<RULES DEFAULT="LITERAL3" SET="JS1">
		<SPAN TYPE="LITERAL4" DELEGATE="javascript::MAIN">
			<BEGIN>:</BEGIN>
			<END>&quot;</END>
		</SPAN>
		<SEQ TYPE="LITERAL1" DELEGATE="OPEN_TAG_ATTRIBUTE_CONTENT">&#32;</SEQ>
		<SEQ TYPE="LITERAL1" DELEGATE="OPEN_TAG_ATTRIBUTE_CONTENT">&#09;</SEQ>
	</RULES>
	<RULES DEFAULT="LITERAL3" SET="JS2">
		<SPAN TYPE="LITERAL4" DELEGATE="javascript::MAIN">
			<BEGIN>:</BEGIN>
			<END>'</END>
		</SPAN>
		<SEQ TYPE="LITERAL1" DELEGATE="OPEN_TAG_ATTRIBUTE_CONTENT">&#32;</SEQ>
		<SEQ TYPE="LITERAL1" DELEGATE="OPEN_TAG_ATTRIBUTE_CONTENT">&#09;</SEQ>
	</RULES>
	
	<RULES DEFAULT="LITERAL3" SET="VALUE">
		<IMPORT DELEGATE="INLINE_PHP"/>
	</RULES>

	<!-- CSS TAG -->
	<RULES DEFAULT="FUNCTION" SET="OPEN_CSS_TAG_ATTRIBUTE_CONTENT">
		<IMPORT DELEGATE="INLINE_PHP"/>
		<SEQ_REGEXP HASH_CHAR="&#32;" TYPE="OPERATOR" DELEGATE="OPEN_CSS_TAG_ATTRIBUTE_VALUE_CONTENT">[\s$]*=[\s$]*</SEQ_REGEXP>
		<SEQ_REGEXP HASH_CHAR="&#09;" TYPE="OPERATOR" DELEGATE="OPEN_CSS_TAG_ATTRIBUTE_VALUE_CONTENT">[\s$]*=[\s$]*</SEQ_REGEXP>
		<SEQ_REGEXP HASH_CHAR="=" TYPE="OPERATOR" DELEGATE="OPEN_CSS_TAG_ATTRIBUTE_VALUE_CONTENT">=[\s$]*</SEQ_REGEXP>
		<SEQ_REGEXP HASH_CHAR="=" TYPE="OPERATOR" DELEGATE="OPEN_CSS_TAG_ATTRIBUTE_VALUE_CONTENT">=</SEQ_REGEXP>
		<SEQ TYPE="MARKUP" DELEGATE="INVALID">/</SEQ>
		<SEQ TYPE="MARKUP" DELEGATE="css::MAIN">&gt;</SEQ>
		<KEYWORDS>
			<KEYWORD2>type</KEYWORD2>
			<KEYWORD2>title</KEYWORD2>
			<KEYWORD2>dir</KEYWORD2>
			<KEYWORD2>lang</KEYWORD2>
			<KEYWORD2>media</KEYWORD2>
		</KEYWORDS>
	</RULES>
	<RULES DEFAULT="LITERAL1" SET="OPEN_CSS_TAG_ATTRIBUTE_VALUE_CONTENT">
		<IMPORT DELEGATE="INLINE_PHP"/>
		<SPAN_REGEXP AT_WORD_START="TRUE" HASH_CHAR="&quot;" TYPE="LITERAL1" DELEGATE="OPEN_TAG_ATTRIBUTE_VALUE_CONTENT1">
			<BEGIN>"[\s$]*</BEGIN>
			<END>"</END>
		</SPAN_REGEXP>
		<SPAN_REGEXP AT_WORD_START="TRUE" HASH_CHAR="'" TYPE="LITERAL1" DELEGATE="OPEN_TAG_ATTRIBUTE_VALUE_CONTENT1">
			<BEGIN>'[\s$]*</BEGIN>
			<END>'</END>
		</SPAN_REGEXP>
		<SEQ TYPE="MARKUP" DELEGATE="css::MAIN">&gt;</SEQ>
		<SEQ TYPE="LITERAL1" DELEGATE="OPEN_CSS_TAG_ATTRIBUTE_CONTENT">&#32;</SEQ>
		<SEQ TYPE="LITERAL1" DELEGATE="OPEN_CSS_TAG_ATTRIBUTE_CONTENT">&#09;</SEQ>
		<SPAN_REGEXP HASH_CHAR="&quot;" TYPE="INVALID">
			<BEGIN>"</BEGIN>
			<END>"</END>
		</SPAN_REGEXP>
		<SPAN_REGEXP HASH_CHAR="'" TYPE="INVALID">
			<BEGIN>'</BEGIN>
			<END>'</END>
		</SPAN_REGEXP>
	</RULES>

